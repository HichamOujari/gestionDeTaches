        <addForeignKeyConstraint baseColumnNames="pole_id"
                                 baseTableName="division"
                                 constraintName="fk_division__pole_id"
                                 referencedColumnNames="id"
                                 referencedTableName="pole"/>
    </changeSet>
</databaseChangeLog>
